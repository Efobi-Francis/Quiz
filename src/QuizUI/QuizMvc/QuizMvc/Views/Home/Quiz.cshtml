@model QuizMvc.Models.QuizAnswer

<h1>@Model.QuizName</h1>

    <form asp-controller="Home" asp-action="Score" method="post">
        @Html.EditorFor(m => m.AttemptId)
        @Html.EditorFor(m => m.QuizId)
        @for (int i = 0; i < Model.Questions.Count; i++)
        {
                @Html.EditorFor(model => model.Questions[i])
        }
        <button type="submit">Score</button>
    </form>

