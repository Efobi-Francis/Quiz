@model QuizMvc.Models.QuizQuestionAnswer

    <p><strong>@Model.QuestionText</strong></p>
    @Html.EditorFor(m => m.QuestionId)
    @for (int i = 0; i < Model.Options.Count(); i++)
    {
        <p><input asp-for="@Model.SelectedOptionId" type="radio" value="@Model.Options[i].OptionId" /> @Model.Options[i].OptionText</p> 
    }

